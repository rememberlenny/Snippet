        <%= image_tag 'maps/bronx-group.png', :class => "bronx list-image" %>
        <%= image_tag 'maps/central-harlem.png', :class => "central list-image" %>
        <%= image_tag 'maps/east-harlem.png', :class => "east list-image" %>
        <%= image_tag 'maps/flushing.png', :class => "flushing list-image" %>
        <%= image_tag 'maps/greenich-village.png', :class => "greenich list-image" %>
        <%= image_tag 'maps/highbridge.png', :class => "highbridge list-image" %>
        <%= image_tag 'maps/jackson-heights.png', :class => "jackson list-image" %>
        <%= image_tag 'maps/lower-east-side.png', :class => "lower list-image" %>
        <%= image_tag 'maps/morningside-heights.png', :class => "morningside list-image" %>
        <%= image_tag 'maps/mott-haven.png', :class => "mott list-image" %>
        <%= image_tag 'maps/north-manhattan-group.png', :class => "north list-image" %>
        <%= image_tag 'maps/pelham-parkway.png', :class => "pelham list-image" %>
        <%= image_tag 'maps/queens-group.png', :class => "queens list-image" %>
        <%= image_tag 'maps/soundview.png', :class => "soundview list-image" %>
        <%= image_tag 'maps/south-manhattan-group.png', :class => "south list-image" %>
        <%= image_tag 'maps/sunnyside.png', :class => "sunnyside list-image" %>
        <%= image_tag 'maps/throgs-neck.png', :class => "throgs list-image" %>
        <%= image_tag 'maps/washington-heights.png', :class => "washington list-image" %>
      </div>
      <style>
      .list-image {
width: 100%;
height: 100%;
}
      .feast{
        opacity: .6;
      }
        .list-image{
          position: absolute;
          left: 0px;
          top: 0;
          visibility: hidden;
          opacity: .4;
        }
        .list-image.east{
          opacity: 1;
        }
        .list-image.visibler{
          visibility: visible;
        }
        h5{
          margin-bottom:0em;
        }
        .no-bullet li{
          opacity:.6;
        }        
        .no-bullet li.list-eastharlem{
          opacity:1;
          font-weight: bold;

        }
        .no-bullet li:hover{
          background: #ccc;
        }
      </style>
   
      <div class="large-3 column text-left">
        <h4>Select your region</h4>
        <ul class="text-left no-bullet">
          <li id="group" class="list-north-manhattan"><h5>North Manhattan</h5> </li>
            <li class=" list-eastharlem"><%= link_to "East Harlem", users_path %></li>
            <li class=" list-centralharlem">Central Harlem (Coming Soon)</li>
            <li class=" list-washingtonheights">Washington Heights (Coming Soon)</li>
            <li class=" list-morningsideheights">Morningside Heights (Coming Soon)</li>
          <li class=" list-southmanhattan" id="group"><h5>South Manhattan</h5> (Coming Soon)</li>
<!--             <li class=" list-financialdistrict">Greenich Village / Financial District (Coming Soon)</li>
            <li class=" list-chinatown">Lower East Side / Chinatown (Coming Soon)</li> -->
          <li class=" list-bronx" id="group"><h5>Bronx</h5> (Coming Soon)</li>
<!--             <li class=" list-highbridge">Highbridge / South Concourse (Coming Soon)</li>
            <li class=" list-motthaven">Mott Haven / Hunts Point (Coming Soon)</li>
            <li class=" list-soundview">Soundview / Parkway (Coming Soon)</li>
            <li class=" list-throg">Throg Neck / Co-Op City (Coming Soon)</li>
            <li class=" list-pelhamparkway">Pelham Parkway (Coming Soon)</li> -->
          <li class=" list-queens" id="group"><h5>Queens</h5>(Coming Soon)</li>
<!--             <li class=" list-astoria">Astoria (Coming Soon)</li>
            <li class=" list-jacksonheights">Jackson Heights (Coming Soon)</li>
            <li class=" list-sunnyside">Sunnyside / Woodside (Coming Soon)</li>
            <li class=" list-flushing">Flushing / Whitestone (Coming Soon)</li> -->
        </ul>
      </div>
    </div>
    <br>
  <% else %>
    <script>
      $('.white-bg').removeClass('white-bg');
      $('.background').css({backgroundImage: 'url("/assets/homeback.jpg")'});
    </script>

    <div class="row ">
      <div class="large-8 column large-centered">
        <div class="large-10 column large-centered">
          <h1 style="font-size: 4em;
          "><%= image_tag 'snipit-logo-grey.png' %></h1>
          <h1 class="subheader">
            Currently publically unavailable.
          </h1>
          <h2 style="font-size:1.3em; color: #333;" class="subheader text-center">
            With Snipit, you can text your group to let them know about free fitness events, healthy recipes and more.
          </h2>
          <%= link_to "Private Access", coach_path, :class=>"button secondary small" %>
        </div>
      </div>
    </div>
          <hr style="visibility: hidden;">
   
  <% end %> 
</div>
<div>
